@using ItLabs.MBox.Application.Models

@{
    Layout = null;
}

@model PagingModel<RecordLabel>

@foreach (RecordLabel rl in Model.PagingList)
{
    <tr class="search">
        <td>
            @rl.User.Name
        </td>
        <td>@rl.User.Email</td>
        <td>@rl.RecordLabelArtists.Count</td>
        <td>
            <button id="@rl.User.Id" class="btn btn-danger" onclick="deleteRecordLabel(this.id)">
                Delete <i class="glyphicon glyphicon-trash"></i>
            </button>
        </td>
    </tr>
}